//******************************
// Declarative pipeline example
//******************************
pipeline {
    agent any

    stages {
        stage('Preparation') {
            steps {
                echo 'Preparing'
            }
        }


        stage('Build') {
            steps {
                echo 'Building'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
